PREFIX dc: <http://purl.org/dc/elements/1.1/>
PREFIX dct: <http://purl.org/dc/terms/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX edm: <http://www.europeana.eu/schemas/edm/>

SELECT distinct ?tijdvak (count(?cho) as ?aantal) {
  ?cho a edm:ProvidedCHO .
  ?cho dc:type/skos:prefLabel ?type .
  ?cho dct:temporal ?tijdvak .
  filter(langMatches(lang(?tijdvak), "nl"))
  filter(langMatches(lang(?type), "nl"))
  filter(contains(?type,"schilderij"))
} order by desc(?aantal) limit 10